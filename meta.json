{
  "prompts": {
    "name": {
      "type": "string",
      "required": true,
      "label": "Project name"
    },
    "description": {
      "type": "string",
      "required": true,
      "label": "Project description",
      "default": "A demo project"
    },
    "isNodeJs": {
      "type": "confirm",
      "label": "Is this a NodeJs project",
      "default": false
    },
    "author": {
      "type": "string",
      "label": "Author"
    },
    "license": {
      "type": "string",
      "label": "License",
      "default": "MIT"
    }
  },
  "completeMessage": "To get started:\n\n  cd {{destDirName}}\n  yarn install\n  yarn run dev"
}